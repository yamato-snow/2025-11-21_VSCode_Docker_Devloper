{
  "name": "nodejs-postgres-react-example",
  "version": "1.0.0",
  "description": "Node.js + PostgreSQL + Redis + React Fullstack Dev Container Example",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "client:dev": "vite client",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "vite build client",
    "start": "node dist/index.js",
    "db:setup": "node init-db.js",
    "db:migrate": "echo 'Database migration placeholder'",
    "type-check": "tsc --noEmit --project client/tsconfig.app.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:client": "vitest run --config client/vitest.config.ts",
    "test:client:watch": "vitest --config client/vitest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm test && npm run test:client && npm run test:e2e"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "redis",
    "react",
    "vite",
    "fullstack",
    "docker",
    "devcontainer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcrypt": "^5.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "vite": "^6.0.0",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "concurrently": "^9.0.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "typescript-eslint": "^8.18.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "vitest": "^1.0.4",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/user-event": "^14.5.2",
    "jsdom": "^23.0.1",
    "@playwright/test": "^1.40.1"
  }
}
